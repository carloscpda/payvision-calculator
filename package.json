{
  "name": "@payvision/frontend-nightswatch-calculator",
  "version": "1.2.2",
  "description": "Code challenge for frontend vacancy at Payvision. REST API exercise. Code review exercise",
  "scripts": {
    "serve": "http-server -p 8082 -o http://localhost:8082/src/index.html -c0",
    "commit": "git-cz",
    "release": "standard-version -a",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "yarn serve & cypress open"
  },
  "author": "Payvision",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-jest": "^26.0.1",
    "cypress": "^4.7.0",
    "cz-conventional-changelog": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^26.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "testMatch": [
      "**/test/unit/**/*.spec.js"
    ]
  }
}